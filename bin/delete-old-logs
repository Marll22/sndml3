#!/bin/bash
# delete log files older than 10 days
echo '#########################'
echo '### DEPRECATED SCRIPT ###'
echo '#########################'
cd $SNDML_HOME/configs
for p in *
do
  dir=$SNDML_HOME/configs/$p/log
  if [[ -d $dir ]]
  then
    find $dir -mtime +10 -type f -print -delete
  fi
done
