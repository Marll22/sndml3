#!/bin/bash
# profile=$1
# target=$2
if [ -n $2 ]
then
  cd $2
fi
dir=`pwd`
url=`awk -F= '/^datamart.url/{print $2}' <$1`
# echo url=$url
db=`echo $url | awk -F: '{print $2}'`
# echo db=$db
case $db in
oracle) tag=ora ;;
postgresql) tag=pg ;;
mysql) tag=mysql ;;
mssql) tag=mssql ;;
esac
# echo tag=$tag
ls $dir/sndml*-$tag.jar | tail -1
