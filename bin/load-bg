#!/bin/bash
#
# Run a load job with all output directed to a file in the log directory.
# This command is intended to be used in cron jobs.
#
echo '*************************'
echo '*** DEPRECATED SCRIPT ***'
echo '*************************'
source sndml-setup
sndml_setup_profile $1
export SNDML_YAML=$SNDML_CONFIG/yaml/$2.yaml
if [[ -z $SNDML_LOG ]]; then
  sdml_set_log $2
fi
env | grep SNDML | sort >$SNDML_LOG
cd $SNDML_CONFIG
java -ea -jar $SNDML_JAR -p $SNDML_PROFILE -y $SNDML_YAML </dev/null >>$SNDML_LOG 2>&1
