#!/bin/bash
source sndml-setup
echo jar=$jar
echo profile=$profile
timestamp=`date +%Y%m%d%H%M`
yamlfile=$sndml_yaml_dir/$1.yaml
logfile="$sndml_log_dir/$1-$timestamp.log"
echo jar=$sndml_jar
echo profile=$sndml_profile
echo yaml=$yamlfile
echo java -ea -jar $jar -p $profile -y $yamlfile "2>&1 >$logfile"
nohup java -ea -jar $jar -p $profile -y $yamlfile 2>&1 >$logfile &
