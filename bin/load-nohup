#!/bin/bash
#
# Run a load job in background using nohup (ignoring hangup signals).
# This command is intended to be run from an interactive terminal session.
#
source sndml-setup
sndml_setup_profile $1
export SNDML_LOG=`logfile-name $1 $2`
echo logfile=$SNDML_LOG
nohup load-log $* >>$SNDML_LOG 2>&1 &
